<v:Screen xmlns="https://github.com/avaloniaui"
          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
          xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
          xmlns:v="using:SimpleMC.Views"
          xmlns:vm="clr-namespace:SimpleMC.ViewModels"
          mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
          x:DataType="vm:InstancesMiddleScreenViewModel"
          x:Class="SimpleMC.Views.InstancesMiddleScreen">
    <UserControl Content="{Binding Screen, Mode=OneTime}" />
</v:Screen>